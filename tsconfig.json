{
	"compilerOptions": {
		"target": "es2020",
		"lib": ["es2020"],
		"module": "es2020",
		"moduleResolution": "node",
		"declaration": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"exclude": ["node_modules"]
}
